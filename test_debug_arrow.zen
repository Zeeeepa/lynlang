{ io, Option } = @std

main = () i32 {
    opt = Option.Some(42)
    
    // Try without arrow syntax first
    result1 = opt ?
        | Option.Some(v) { v }
        | Option.None { 0 }
    
    io.println("Result1: ${result1}")
    
    // Now try with arrow syntax
    result2 = opt ?
        | Option.Some(v) => v
        | Option.None => 0
    
    io.println("Result2: ${result2}")
    
    result1
}
// Test nested generics inside void function
{ io, Result } = @std

test_func = () void {
    io.println("Testing in void function")
    
    get_nested = () Result<Result<i32, string>, string> {
        Result.Ok(Result.Ok(42))
    }
    
    result = get_nested()
    inner = result.raise()
    value = inner.raise()
    
    io.println("Value: ${value}")
}

main = () void {
    test_func()
}
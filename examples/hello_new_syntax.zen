// Example showing the correct import syntax
// Imports are at module level, NOT in comptime blocks

core := @std.core
build := @std.build
io := build.import("io")

main = () i32 {
    io.print("Hello, Zen!\n")
    io.print("This example uses the correct import syntax.\n")
    io.print("Imports are at module level, not in comptime blocks.\n")
    return 0
}
extern printf = (format: *i8, ...) i32

main = () i32 {
    // Test just the inner conditional with x != 0 case
    y := 0
    
    // This should give 2 when y == 0
    inner_result := y == 0 ? | true => 2 | false => 3
    
    printf("inner_result with y=0: %d (expected 2)\n", inner_result)
    
    // Now with y = 1
    y = 1  
    inner_result2 := y == 0 ? | true => 2 | false => 3
    printf("inner_result with y=1: %d (expected 3)\n", inner_result2)
    
    return 0
}
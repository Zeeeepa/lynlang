// Debug test for raise with nested generics
{ io, Result } = @std

main = () void {
    // Test 1: Direct raise on nested Result
    double = Result.Ok(Result.Ok(42))
    
    // First raise should extract Result<i32, string>
    inner = double.raise()
    
    // Second raise should extract i32
    value = inner.raise()
    
    io.println("Value from nested raise: ${value}")
    
    // Test 2: Using a function
    get_nested = () Result<Result<i32, string>, string> {
        Result.Ok(Result.Ok(100))
    }
    
    nested = get_nested()
    inner2 = nested.raise()
    value2 = inner2.raise()
    
    io.println("Value from function: ${value2}")
}
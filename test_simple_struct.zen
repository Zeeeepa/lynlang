// Simple struct test
io = @std

Point: {
    x: i32,
    y: i32,
}

main = () i32 {
    io.println("Testing simple struct...")
    
    // Try creating a struct with ::=
    p ::= Point { x: 10, y: 20 }
    
    io.print("Point x = ")
    io.print_int(p.x)
    io.println("")
    
    io.print("Point y = ")
    io.print_int(p.y) 
    io.println("")
    
    io.println("Struct test complete!")
    return 0
}
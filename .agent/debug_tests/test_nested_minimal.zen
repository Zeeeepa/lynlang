// Minimal nested Result test
{ io, Result } = @std

main = () i32 {
    // Create the simplest nested Result
    inner = Result.Ok(42)
    outer = Result.Ok(inner)
    
    // Extract and print
    outer ?
        | Result.Ok(extracted) {
            extracted ?
                | Result.Ok(val) {
                    io.println("Value: ${val}")
                    val
                }
                | Result.Err(e) => 1
        }
        | Result.Err(e) => 2
}
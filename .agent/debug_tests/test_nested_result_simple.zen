// Test nested Result type extraction
{ io, Result } = @std

// Function that returns Result<Result<i32, string>, string>
get_nested = () Result<Result<i32, string>, string> {
    Result.Ok(Result.Ok(42))
}

main = () i32 {
    // Get the nested result
    nested = get_nested()
    
    // Check what we got
    nested ?
        | Result.Ok(inner) {
            io.println("Got inner result")
            inner ?
                | Result.Ok(val) {
                    io.println("Got value: ${val}")
                    val == 42 ?
                        | true { 0 }
                        | false { 1 }
                }
                | Result.Err(e) {
                    io.println("Inner error: ${e}")
                    2
                }
        }
        | Result.Err(e) {
            io.println("Outer error: ${e}")
            3
        }
}
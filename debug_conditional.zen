// Debug conditional parsing
extern printf = (format: *i8, ...) i32

test1 = (n: i32) i32 {
    // Works: assign then return
    x := n > 5 ? | true => 10 | false => 20
    return x
}

test2 = (n: i32) i32 {
    // Fails?: direct return of conditional
    return n > 5 ? | true => 10 | false => 20
}

main = () i32 {
    printf("test1(3) = %d\n", test1(3))
    printf("test2(3) = %d\n", test2(3))
    return 0
}
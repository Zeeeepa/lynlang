// Simple test for nested generics type tracking
{ io, Result, Option } = @std

main = () void {
    // Test 1: Direct nested construction
    nested = Result.Ok(Result.Ok(42))
    io.println("Created nested result")
    
    // Test 2: Function returning nested
    get_nested = () Result<Result<i32, string>, string> {
        Result.Ok(Result.Ok(100))
    }
    
    result = get_nested()
    io.println("Got result from function")
    
    // Try to extract
    inner = result.raise()
    io.println("Extracted inner")
    
    value = inner.raise()
    io.println("Value: ${value}")
}
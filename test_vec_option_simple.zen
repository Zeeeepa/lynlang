// Test Vec with Option
{ io, Option } = @std

main = () void {
    io.println("Testing Vec<Option<i32>, 3>")
    
    // Create Vec of Option<i32>
    vec = Vec<Option<i32>, 3>()
    io.println("Vec created")
    
    // Create a Some value
    some_val = Option.Some(100)
    io.println("Created Some(100)")
    
    // Try to push it
    vec.push(some_val)
    io.println("Pushed Some(100) to vec")
    
    io.println("Test complete")
}

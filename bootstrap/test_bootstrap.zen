// Test file for bootstrap compiler
// Simple program to test basic compilation

io := @std.io

// Fibonacci function
fib = (n: i32) i32 {
    n <= 1 ?
        | true => { return n }
        | false => {
            a := fib(n - 1)
            b := fib(n - 2)
            return a + b
        }
}

// Main function
main = () i32 {
    io.print("Testing Bootstrap Compiler\n")
    
    result := fib(10)
    io.print("fib(10) = ")
    io.print_int(result)
    io.print("\n")
    
    return 0
}
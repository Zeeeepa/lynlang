// Debug arrow syntax issue
{ io, Option } = @std

main = () i32 {
    // Test 1: Direct Some with arrow
    opt1 = Option.Some(5)
    res1 = opt1 ?
        | Option.Some(x) => x + 10
        | Option.None => 42
    io.println("Result 1: ${res1}")
    
    // Test 2: Direct None with literal value in arrow  
    opt2 = Option.None
    res2 = opt2 ?
        | Option.Some(x) => 100  // Don't use x
        | Option.None => 42
    io.println("Result 2: ${res2}")
    
    0
}
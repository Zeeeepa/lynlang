// Test only HashMap get with Option pattern matching
{ io, HashMap, Option , get_default_allocator } = @std

main = () i32 {
    map ::= HashMap<i32, i32>.new()
    
    // Simple functions
    hash_fn = (x: i32) i64 { 16 as i64 , get_default_allocator }
    eq_fn = (a: i32, b: i32) i64 { a == b ? | true { 1 as i64 } | false { 0 as i64 } }
    
    // Insert and get
    map.insert(5, 999, hash_fn, eq_fn)
    
    result ::= map.get(5, hash_fn, eq_fn)
    
    // Test the result from HashMap
    result ?
        | Option.Some(v) { 
            io.println("HashMap returned: ${v, get_default_allocator }") 
            return 0
        }
        | Option.None { 
            io.println("HashMap returned None") 
            return 0
        , get_default_allocator }
}
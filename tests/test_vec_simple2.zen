// Test to verify value extraction in nested generics - simplified
{ io, Result , get_default_allocator} = @std

main = () i32 {
    alloc = get_default_allocator()
    // Test nested without comparisons
    inner = Result.Ok(100)
    outer = Result.Ok(inner)
    
    outer ?
        | Result.Ok(inner_result) {
            io.println("Outer extracted")
            inner_result ?
                | Result.Ok(v) { 
                    // Just print, don't compare
                    io.println("Inner value extracted - trying to print")
                    io.println("Value: ${v}")
                }
                | Result.Err(e) { io.println("Inner err") }
        }
        | Result.Err(e) { io.println("Outer err") }
    
    0
}
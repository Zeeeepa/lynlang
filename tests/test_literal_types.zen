// Test literal types
{ io , get_default_allocator} = @std

test_int = () i32 {
    return 42
}

test_i64 = () i64 { 
    x ::= 42 as i64
    return x
}

test_bool = () bool {
    return true
}

main = () i32 {
    alloc = get_default_allocator()
    a ::= test_int()
    io.println("i32: ${a}")
    
    b ::= test_i64()
    io.println("i64: ${b}")
    
    c ::= test_bool()
    io.println("bool: ${c}")
    
    return 0
}

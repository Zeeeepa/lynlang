// Test for HashMap.remove() with pattern matching
{ io } = @std
{ HashMap } = @std

main = () i32 {
    // Create a HashMap
    map = HashMap<i32, i32>.new()
    
    // Add one value
    map.insert(5, 100)
    
    // Remove the key and pattern match
    io.println("About to call remove...")
    result = map.remove(5)
    io.println("Remove returned, about to pattern match...")
    
    result ?
        | Option.Some(value) { io.println("Removed value: ${value}") }
        | Option.None { io.println("Key not found") }
    
    io.println("Pattern match complete!")
    0
}
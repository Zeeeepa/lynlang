// Test closure with division
{ io, Result } = @std

main = () i32 {
    divide = (a: f64, b: f64) Result<f64, string> {
        return Result.Ok(a / b)
    }
    
    val = divide(10.0, 2.0).raise()
    
    // Pattern match with return in false branch
    val == 5.0 ?
        | true { io.println("Pass") }
        | false {
            io.println("Fail")
            return 1
        }
    
    return 0
}
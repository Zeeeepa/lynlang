// Test all collections with operations
{ io, get_default_allocator, HashMap, DynVec } = @std

main = () void {
    io.println("Testing all collections with operations")
    
    // Get allocator
    alloc = get_default_allocator()
    
    // Create all collections
    map = HashMap<i32, i32>(alloc)
    vec = DynVec<i32>(alloc)
    arr: Array<i32> = Array.new(alloc, 10, 0)
    io.println("All collections created")
    
    // Test operations
    io.println("Testing HashMap insert")
    map.insert(1, 100)
    io.println("HashMap insert OK")
    
    io.println("Testing DynVec push")
    vec.push(42)
    io.println("DynVec push OK")
    
    io.println("Testing Array push")
    arr.push(99)
    io.println("Array push OK")
    
    io.println("All operations completed successfully")
}
// Debug Array issue
{io, get_default_allocator, Array} = @std

main = () void {
    io.println("Creating array...")
    alloc = get_default_allocator()
    arr: Array<i32> = Array.new(alloc, 10, 0)
    
    io.println("Pushing...")
    arr.push(42)
    
    io.println("Getting index 0...")
    val = arr.get(0)
    io.println("Got: ${val}")
    
    io.println("Done")
}
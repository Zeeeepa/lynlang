// Debug Array issue
{ io, Array } = @std

main = () void {
    io.println("Creating array...")
    arr: Array<i32> = Array.new(10, 0)
    
    io.println("Pushing...")
    arr.push(42)
    
    io.println("Getting index 0...")
    val = arr.get(0)
    io.println("Got: ${val}")
    
    io.println("Done")
}
{ io, Result , get_default_allocator} = @std

// Simple function returning Result<void, string>
test_fn = () Result<void, string> {
    Result.Ok(void)
}

main = () i32 {
    alloc = get_default_allocator()
    result = test_fn()
    
    result ?
        | Result.Ok(_) { io.println("Got Ok") }
        | Result.Err(e) { io.println("Got Err: ${e}") }
    
    0
}
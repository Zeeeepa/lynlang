// Debug Option pattern matching
{ io } = @std

main = () i32 {
    x = Some(42)
    
    // Test without extraction first
    result = x ? 
        | .Some => 100
        | .None => -1
    
    io.println("Pattern matched without extraction: ${result}")
    
    // Now test with extraction
    result2 = x ? 
        | Some(v) => {
            io.println("Inside Some branch")
            v
        }
        | None => -1
    
    io.println("Value extracted: ${result2}")
    
    0
}
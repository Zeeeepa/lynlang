// Test closure params with raise and pattern
{ io, Result , get_default_allocator} = @std

main = () i32 {
    alloc = get_default_allocator()
    test = (x: f64) Result<f64, string> {
        return Result.Ok(x)
    }
    
    val = test(5.0).raise()
    
    // Pattern match with return in false branch
    val == 5.0 ?
        | true { io.println("Pass") }
        | false {
            io.println("Fail")
            return 1
        }
    
    return 0
}
// Test if generic type context gets polluted within a function
{ io, Result } = @std

test_mixed = () void {
    io.println("Testing mixed types in one function:")
    
    // First: string payload
    str_result = Result.Ok("hello")
    str_result ?
        | Result.Ok(s) { io.println("  String: ${s}") }
        | Result.Err(e) { io.println("  Error: ${e}") }
    
    // Second: integer payload (might be affected by previous string)
    int_result = Result.Ok(314159)
    int_result ?
        | Result.Ok(i) { io.println("  Integer: ${i}") }
        | Result.Err(e) { io.println("  Error: ${e}") }
}

main = () i32 {
    io.println("=== Testing Function Context Pollution ===")
    test_mixed()
    0
}
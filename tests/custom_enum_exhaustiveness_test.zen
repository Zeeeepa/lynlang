// Test custom enum exhaustiveness checking

Color enum {
    Red
    Green
    Blue
}

Status enum {
    Active
    Inactive
    Pending
}

test_incomplete_color = (color: Color) i32 {
    // Should warn: missing Green and Blue
    color ?
        | Red { 1 }
}

test_complete_color = (color: Color) i32 {
    // Should NOT warn - all variants covered
    color ?
        | Red { 1 }
        | Green { 2 }
        | Blue { 3 }
}

test_wildcard_color = (color: Color) i32 {
    // Should NOT warn - wildcard catches all
    color ?
        | Red { 1 }
        | _ { 0 }
}

test_incomplete_status = (s: Status) i32 {
    // Should warn: missing Pending
    s ?
        | Active { 1 }
        | Inactive { 0 }
}

main = () i32 {
    return 0
}

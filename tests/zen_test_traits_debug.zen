// Debug test for trait implementations

{ io } = @std

// Simple trait
Printable: {
    print: (self) void,
}

// Simple struct
Message: {
    text: string,
}

// Implement trait
Message.implements(Printable, {
    print = (self) void {
        io.println("Message: ${self.text}")
    },
})

main = () void {
    msg = Message { text: "Hello from trait!" }
    msg.print()
    
    io.println("Direct access: ${msg.text}")
}
// Test loop with index
{ io } = @std
{ Vec } = @std

main = () void {
    io.println("Testing loop with index:")
    
    // Create a simple vector
    items = Vec<i32, 5>()
    items.push(10)
    items.push(20)
    items.push(30)
    
    // Test loop with index
    items.loop((item, i) {
        io.println("Item ${i}: ${item}")
    })
}
// Simple nested generics test
{ io, Result , get_default_allocator} = @std

main = () void {
    alloc = get_default_allocator()
    io.println("Testing nested Result")
    
    // Create nested Result directly
    nested = Result.Ok(Result.Ok(42))
    
    io.println("Created nested result")
    
    // Try to extract the inner Result
    inner_result = nested.raise()
    
    io.println("Got inner result")
    
    // Extract the final value
    value = inner_result.raise()
    
    io.println("Final value: ${value}")
}
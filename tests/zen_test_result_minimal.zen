// Minimal Result type test
{ io } = @std

// Simple function returning Result  
check_positive = (n: i32) Result<i32, string> {
    n > 0 ?
        | true { return Ok(n) }
        | false { return Err("Not positive") }
}

main = () void {
    result = check_positive(5)
    result ?
        | Ok(val) { io.println("Got: ${val}") }
        | Err(msg) { io.println("Error: ${msg}") }
}

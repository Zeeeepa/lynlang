// Minimal Result test
{ io } = @std

Result<T, E>: Ok(T) | Err(E)

test_result = () Result<i32, string> {
    return Ok(42)
}

main = () i32 {
    result = test_result()
    
    result ?
        | Ok(val) { 
            io.println("OK: ${val}")
            return 0
        }
        | Err(err) {
            io.println("Error: ${err}")
            return 1
        }
}
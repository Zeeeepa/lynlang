{ io, Option , get_default_allocator} = @std

main = () void {
    alloc = get_default_allocator()
    s = "3.14159"
    // Explicitly type the variable  
    opt = s.to_f64()
    
    opt ?
        | Option.Some(v) {
            doubled = v * 2.0
            io.println("3.14159 * 2.0 = ${doubled}")
        }
        | Option.None {
            io.println("Parse failed")
        }
}
// Test trait implementation
{ io, math } = @std

// Trait definition
Geometric: {
    area: (self) f64
}

// Circle struct
Circle: {
    radius: f64
}

// Test trait implementation
Circle.implements(Geometric, {
    area = (self) f64 {
        return 3.14159 * self.radius * self.radius
    }
})

main = () void {
    circle = Circle { radius: 5.0 }
    io.println("Circle area: ${circle.area()}")
}
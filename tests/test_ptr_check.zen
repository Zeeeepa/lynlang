// Test to check pointers
{ io } = @std

main = () i32 {
    str1 = "hello"
    upper1 = str1.to_upper()
    io.println("After first to_upper")
    io.println(upper1)
    
    str2 = "WORLD"
    lower1 = str2.to_lower()
    io.println("After first to_lower")  
    io.println(lower1)
    
    io.println("Doing comparison 1")
    ok1 = upper1 == "HELLO"
    io.println("Comparison 1 done")
    
    io.println("Doing comparison 2")
    ok2 = lower1 == "world"
    io.println("Comparison 2 done")
    
    return 0
}
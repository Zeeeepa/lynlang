// Simple test for raise() with f64
{ io, Result } = @std

test_f64 = () Result<f64, string> {
    a = Result.Ok(5.0)
    val = a.raise()
    
    // Just try to use val as f64 without arithmetic first
    io.println("Value extracted: ${val}")
    
    return Result.Ok(val)
}

main = () i32 {
    result = test_f64()
    result ?
        | Result.Ok(v) { 
            io.println("Success: got f64")
            return 0
        }
        | Result.Err(e) { 
            io.println("Error: ${e}")
            return 1
        }
}
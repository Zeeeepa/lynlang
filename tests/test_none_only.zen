// Test just None variable creation and match
{ io , get_default_allocator} = @std

main = () void {
    alloc = get_default_allocator()
    empty: Option<i32> = None
    io.println("Created None")
    
    empty ?
        | None { io.println("Is None") }
        | Some(x) { io.println("Is Some: ${x}") }
}
// Test calling trait method
{ io } = @std

// Trait  
Display: {
    show: (self) void,
}

// Type
Item: {
    name: String,
}

// Implementation
Item.implements(Display, {
    show = (self) void {
        io.println("Showing item")
    },
})

main = () void {
    item ::= Item { name: "test" }
    item.show()  // Call trait method
}
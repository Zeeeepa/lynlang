// Test raise with pattern return
{ io, Result } = @std

main = () i32 {
    get_val = () Result<f64, string> {
        return Result.Ok(5.0)
    }
    
    val = get_val().raise()
    
    // Pattern match with return in false branch
    val == 5.0 ?
        | true { io.println("Pass") }
        | false {
            io.println("Fail")
            return 1
        }
    
    return 0
}
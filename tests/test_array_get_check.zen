{io, get_default_allocator, Array} = @std

main = () i32 {
    alloc = get_default_allocator()
    arr: Array<i32> = Array.new(alloc, 10, 0)
    arr.push(100)
    
    val = arr.get(0)
    
    // Compare the value
    val == 100 ? 
        | true { io.println("Got correct value: 100") }
        | false { io.println("Got incorrect value") }
    
    return 0
}
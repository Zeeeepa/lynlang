// Test file for LSP features

{ Result } = @std

// Test Rename: Try renaming "divide" across files
divide = (a: f64, b: f64) Result<f64, StaticString> {
    return Result.Ok(a / b)
}

// Test Signature Help: Type "divide(" and see parameter info
// Test Inlay Hints: Variable 'result' should show ": Result<f64, StaticString>"
main = () i32 {
    result = divide(10.0, 2.0)

    result ?
        | Ok(val) { println("Success") }
        | Err(msg) { println("Error") }

    return 0
}

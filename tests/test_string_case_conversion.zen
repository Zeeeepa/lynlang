{ io } = @std

main = () i32 {
    text = "Hello World 123!"
    upper = text.to_upper()
    lower = text.to_lower()
    
    io.println("Original: ${text}")
    io.println("Uppercase: ${upper}")
    io.println("Lowercase: ${lower}")
    
    // Test with mixed case
    mixed = "AbCdEfG"
    io.println("Mixed: ${mixed}")
    io.println("Upper: ${mixed.to_upper()}")
    io.println("Lower: ${mixed.to_lower()}")
    
    return 0
}
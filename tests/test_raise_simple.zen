// Minimal test for .raise() functionality
{ io, Result, Ok, Err } = @std

test_raise = () Result<i32, string> {
    // Create a Ok with an integer
    result = Ok(42)
    
    // Use .raise() to extract the value - if Err, propagates automatically
    value = result.raise()
    
    // Return Ok with the extracted value
    return Ok(value)
}

main = () i32 {
    result = test_raise()
    result ?
        | Ok(val) { 
            io.println("Got Ok variant")
            return 42
        }
        | Err(e) { 
            io.println("Got Err variant")
            return 1
        }
}
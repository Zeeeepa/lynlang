// Simple HashSet test to verify basic functionality
{ io, HashSet, get_default_allocator} = @std

main = () i32 {
    alloc = get_default_allocator()
    // Test HashSet creation
    set = HashSet<i32>(alloc)
    
    // Test add operation
    added = set.add(10)
    added ? 
        | true { io.println("Added 10 to set") }
        | false { io.println("Failed to add 10") }
    
    // Test contains
    has10 = set.contains(10)
    has10 ?
        | true { io.println("Set contains 10") }
        | false { io.println("Set does not contain 10") }
    
    // Test size
    size = set.size()
    io.println("Set size: ${size}")
    
    0
}
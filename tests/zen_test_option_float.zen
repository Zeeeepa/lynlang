// Test Option type with float
{ io } = @std

Option<T>: Some(T) | None

main = () void {
    io.println("=== Testing Option Float ===")
    
    // Test Some with float
    maybe_radius: Option<f64> = Some(5.5)
    maybe_radius ?
        | Some(r) { io.println("Got radius: ${r}") }
        | None { io.println("No radius") }
    
    io.println("=== Complete ===")
}
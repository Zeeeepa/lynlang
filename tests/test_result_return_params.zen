// Test Result return with function parameters
{ io, Result } = @std

// Function with parameters returning Result
divide = (a: i32, b: i32) Result<i32, string> {
    b == 0 ? 
        | true { return Result.Err("Division by zero") }
        | false { return Result.Ok(a / b) }
}

main = () i32 {
    io.println("Testing Result return with parameters:")
    
    result1 = divide(10, 2)
    result1 ?
        | .Ok(val) { io.println("10 / 2 = ${val}") }
        | .Err(err) { io.println("Error: ${err}") }
    
    result2 = divide(10, 0)
    result2 ?
        | .Ok(val) { io.println("10 / 0 = ${val}") }
        | .Err(err) { io.println("Error: ${err}") }
    
    return 0
}
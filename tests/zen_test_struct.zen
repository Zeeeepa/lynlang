// Test struct definitions and literals
{ io } = @std

// Define a Point struct
Point: {
    x:: f64,
    y:: f64 = 0
}

// Define a Circle struct
Circle: {
    center: Point,
    radius: f64
}

main = () void {
    io.println("=== Struct Test ===")
    
    // Create struct literals
    p1 = Point { x: 10.5, y: 20.5 }
    io.println("Point created!")
    
    // Create nested struct
    c = Circle {
        center: Point { x: 0, y: 0 },
        radius: 5.0
    }
    io.println("Circle created!")
    
    io.println("Test completed")
}

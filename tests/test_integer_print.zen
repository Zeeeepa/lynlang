// Test integer printing directly
{ io } = @std

main = () i32 {
    io.println("=== Testing Integer Printing ===")
    
    // Direct integers
    io.println("Direct 314159: ${314159}")
    io.println("Direct 1000000: ${1000000}")
    
    // Via variables
    a = 314159
    b = 1000000
    io.println("Variable a=314159: ${a}")
    io.println("Variable b=1000000: ${b}")
    
    0
}
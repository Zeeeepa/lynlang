// Minimal test case for closure type issue
{ io, Result } = @std

main = () i32 {
    // This closure has explicit return type
    test = () Result<i32, string> {
        return Result.Ok(42)
    }
    
    val = test().raise()
    
    // This comparison is failing  
    val == 42 ?
        | true { io.println("Success") }
        | false { io.println("Failed") }
    
    return 0
}
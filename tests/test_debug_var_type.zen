// Test variable type tracking
{ io, Result } = @std

get_nested = () Result<Result<i32, string>, string> {
    Result.Ok(Result.Ok(42))
}

main = () void {
    io.println("Testing variable type tracking")
    
    // Get inner Result explicitly typed
    inner: Result<i32, string> = get_nested().raise()
    io.println("Got inner (explicit type)")
    
    value = inner.raise()
    io.println("Value: ${value}")
}
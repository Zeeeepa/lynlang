// Minimal trait implementation test
{ io } = @std

// Trait
Display: {
    show: (self) void,
}

// Type
Item: {
    name: String,
}

// Implementation - simplified version
Item.implements(Display, {
    show = (self) void {
        io.println("Item")
    },
})

main = () void {
    io.println("Implementation compiled")
}
// Test simple nested generic payload extraction
{ io, Result, Option } = @std

main = () i32 {
    io.println("Testing simple nested generic")
    
    // Create inner Result.Ok(42)
    inner = Result.Ok(42)
    
    // Create outer Result.Ok(inner)
    outer = Result.Ok(inner)
    
    // Extract outer then inner
    outer ?
        | Result.Ok(inner_result) {
            io.println("Extracted inner Result from outer")
            inner_result ?
                | Result.Ok(val) {
                    io.println("Extracted value: ${val}")
                    val == 42 ?
                        | true { io.println("SUCCESS: Got 42") }
                        | false { io.println("FAIL: Got ${val} instead of 42") }
                }
                | Result.Err(e) { io.println("Inner was Err") }
        }
        | Result.Err(e) { io.println("Outer was Err") }
    
    0
}
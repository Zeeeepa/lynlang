// Simple HashMap test to isolate issues
{ io, HashMap } = @std

main = () i32 {
    io.println("Testing HashMap basic instantiation...")
    
    // Create HashMap<string, i32>
    map ::= HashMap<string, i32>.new()
    io.println("HashMap created successfully")
    
    // Test len() method
    size ::= map.len()
    io.println("Map size: ${size}")
    
    // Test is_empty() method  
    empty ::= map.is_empty()
    io.println("Map is empty: ${empty}")
    
    return 0
}

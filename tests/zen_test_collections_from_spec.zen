// Test collections from LANGUAGE_SPEC.zen lines 374-385

{ io } = @std
{ Vec, DynVec } = @std

main = () void {
    // Static sized vector
    shapes = Vec<Shape, 10>()
    shapes.push(42)
    
    // Dynamic vector  
    numbers = DynVec<i32>()
    numbers.push(1)
    numbers.push(2)
    numbers.push(3)
    
    // Loop over collection
    numbers.loop((n) {
        io.println("Number: ${n}")
    })
    
    // Loop with index
    numbers.loop((n, i) {
        io.println("${i}: ${n}")
    })
}
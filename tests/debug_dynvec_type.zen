// Test DynVec instantiation
{io, DynVec, get_default_allocator} = @std

main = () i32 {
    alloc = get_default_allocator()
    
    // Create DynVec with explicit type
    vec = DynVec<i32>(alloc)
    
    // Push a value
    vec.push(42)
    
    // Try getting length
    len = vec.len()
    io.print_int(len)
    
    return 0
}
// Debug test for Option<T> payload
{ io } = @std

Option<T>: Some(T) | None

main = () void {
    // Test with direct integer literal
    x = 42
    io.println("Direct value x: ${x}")
    
    // Test with Option
    maybe: Option<i32> = Some(42)
    maybe ?
        | Some(v) { 
            io.println("Extracted v from Some(42)")
            // The v value should be 42 but it's showing as garbage
        }
        | None { 
            io.println("Was None") 
        }
}

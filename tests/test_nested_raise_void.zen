// Test raise in void function
{ io, Result , get_default_allocator} = @std

get_nested = () Result<Result<i32, StaticString>, StaticString> {
    Result.Ok(Result.Ok(42))
}

test_raises = () void {
    io.println("In void function...")
    
    outer = get_nested()
    // This should fail - raise in void function
    inner = outer.raise()
    io.println("This shouldn't print")
}

main = () void {
    alloc = get_default_allocator()
    test_raises()
}
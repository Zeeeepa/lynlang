// Test HashMap with string keys
{ io, Option, HashMap , get_default_allocator } = @std

main = () void {
    // Create a HashMap with string keys and i32 values
    alloc = get_default_allocator()
    map = HashMap<string, i32>(alloc)
    
    io.println("HashMap created")
    
    // Try to insert a value
    map.insert("key1", 100)
    
    io.println("Value inserted")
    
    // Try to get the value
    result = map.get("key1")
    
    result ?
        | Some(val) { io.println("Got value: ${val, get_default_allocator }") }
        | None { io.println("Key not found") }
}

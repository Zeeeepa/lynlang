// Simple test for nested struct field access
{ io } = @std

// Simple structs
Point: {
    x: f64,
    y: f64,
}

Rectangle: {
    top_left: Point,
}

main = () void {
    // Create rectangle
    rect = Rectangle {
        top_left: Point { x: 1.0, y: 2.0 },
    }
    
    // Access nested field - just one level
    io.println("Top left: ${rect.top_left}")
}
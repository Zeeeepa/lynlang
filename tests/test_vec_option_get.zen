// Test Vec.get with Option
{ io, Option , get_default_allocator} = @std

main = () void {
    alloc = get_default_allocator()
    io.println("Testing Vec<Option<i32>, 3> get")
    
    // Create Vec of Option<i32>
    vec = Vec<Option<i32>, 3>()
    
    // Push a Some value
    vec.push(Option.Some(100))
    io.println("Pushed Some(100)")
    
    // Get it back with explicit type
    opt: Option<i32> = vec.get(0)
    io.println("Got element from vec")
    
    // Pattern match
    opt ?
        | Some(val) { io.println("Value: ${val}") }
        | None { io.println("None") }
}

// Test trait implementation
{ io } = @std

// Trait definition
Printable: {
    print: (self) void,
}

// Struct definition
MyStruct: {
    value: i32,
}

// Implement trait for struct
MyStruct.implements(Printable, {
    print = (self) void {
        io.println("Value: ${self.value}")
    },
})

main = () void {
    obj = MyStruct { value: 42 }
    obj.print()
}
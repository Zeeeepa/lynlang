// Test multiple Option<T> in sequence
{ io } = @std

main = () i32 {
    io.println("Test 1: Some(42)")
    x = Some(42)
    x ?
        | .Some(v) {
            io.println("Got: ${v}")
        }
        | .None {
            io.println("None")
        }
    
    io.println("Test 2: None")
    y = None
    y ?
        | .Some(v) {
            io.println("Got: ${v}")
        }
        | .None {
            io.println("Got None")
        }
    
    io.println("Test 3: Some(100)")  
    z = Some(100)
    z ?
        | .Some(v) {
            io.println("Got: ${v}")
        }
        | .None {
            io.println("None")
        }
    
    io.println("All tests done")
    0
}
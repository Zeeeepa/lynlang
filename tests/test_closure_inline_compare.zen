// Test inline closure comparison
{ io, Result , get_default_allocator} = @std

main = () i32 {
    alloc = get_default_allocator()
    // Don't store closure in variable - call directly
    f = (a: i32, b: i32) { return Result.Ok(a + b) }
    val = f(10, 5).raise()
    io.println("Value: ${val}")
    
    // Now compare
    x = val == 15
    io.println("Comparison: ${x}")
    
    return 0
}
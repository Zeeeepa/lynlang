// Simple DynVec test
{ io } = @std

main = () i32 {
    // Create a DynVec with initial capacity
    // For now, pass 0 as dummy allocator  
    vec = DynVec<i32>(0, 10)
    
    // Test push
    vec.push(42)
    io.println("Pushed 42")
    
    // Test len
    length = vec.len()
    io.println("Length: ${length}")
    
    // Test get
    first = vec.get(0)
    first ?
        | Some(val) { io.println("First element: ${val}") }
        | None { io.println("No element at index 0") }
    
    return 0
}
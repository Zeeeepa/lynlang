{ io, Option , get_default_allocator} = @std

main = () void {
    alloc = get_default_allocator()
    s = "3.14159"
    result = s.to_f64()
    
    result ?
        | Option.Some(v) { 
            // Just print the value directly
            io.println("Value: ${v}")
        }
        | Option.None { 
            io.println("Parse failed")
        }
}
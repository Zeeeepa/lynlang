// Test method chaining on nested results
{ io, Result , get_default_allocator} = @std

main = () void {
    alloc = get_default_allocator()
    io.println("Test: Method chaining")
    
    get_chained = () Result<Result<i32, StaticString>, StaticString> {
        Result.Ok(Result.Ok(500))
    }
    
    // Chain multiple raise() calls
    value = get_chained().raise().raise()
    
    io.println("Got value: ${value}")
}
// Test basic StaticString operations with new type system
{ io } = @std

main = () i32 {
    // StringLiteral (compile-time)
    literal: StaticString = "Hello"
    
    // StaticString 
    static_str: StaticString = "World"
    
    // Test StaticString operations
    io.println(literal)
    io.println(static_str)
    
    return 0
}
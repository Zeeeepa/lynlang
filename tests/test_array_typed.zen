// Test Array with explicit typing
{ io, Array , get_default_allocator} = @std

main = () void {
    alloc = get_default_allocator()
    // Try with explicit type annotation
    arr: Array<i32> = Array<i32>()
    io.println("Array created")
    
    arr.push(42)
    io.println("Pushed value")
    
    val = arr.get(0)
    io.println("Value: ${val}")
}
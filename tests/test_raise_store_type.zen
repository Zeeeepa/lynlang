// Test type of raise result when stored
{ io, Result } = @std

main = () i32 {
    add = (a: i32, b: i32) {
        return Result.Ok(a + b)
    }
    
    // Store raise result in variable with explicit type
    val: i32 = add(10, 5).raise()
    io.println("Value: ${val}")
    
    // Now compare
    x = val == 15
    io.println("Comparison: ${x}")
    
    return 0
}
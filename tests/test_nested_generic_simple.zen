// Test nested generic type: Result<Result<i32, StaticString>, StaticString>
{ io, Result , get_default_allocator} = @std

test_nested = () Result<Result<i32, StaticString>, StaticString> {
    Result.Ok(Result.Ok(100))
}

main = () void {
    alloc = get_default_allocator()
    io.println("Testing nested generic...")
    
    outer = test_nested()
    inner = outer.raise()
    val = inner.raise()
    
    io.println("Got value: ${val}")
    
    val == 100 ?
        | true { io.println("✓ Works!") }
        | false { io.println("✗ Failed, got ${val}") }
}

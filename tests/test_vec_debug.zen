// Debug test for inspecting values
{ io, Result } = @std

main = () i32 {
    // Direct Result.Ok with integer
    direct_ok = Result.Ok(42)
    
    direct_ok ?
        | Result.Ok(val) {
            io.println("Direct extraction: ${val}")
        }
        | Result.Err(e) {
            io.println("Error")
        }
    
    0
}
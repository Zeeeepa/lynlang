// Test DynVec and Array together
{ io, get_default_allocator, DynVec } = @std

main = () void {
    io.println("Testing DynVec and Array")
    
    alloc = get_default_allocator()
    
    vec = DynVec<i32>(alloc)
    arr: Array<i32> = Array.new(alloc, 10, 0)
    
    vec.push(42)
    arr.push(99)
    
    io.println("Both operations completed")
}
// Test simple generic type: Result<i32, string>
{ io, Result } = @std

test_result = () Result<i32, string> {
    Result.Ok(42)
}

main = () void {
    io.println("Testing simple generic...")
    
    result = test_result()
    val = result.raise()
    
    io.println("Got value: ${val}")
    
    val == 42 ?
        | true { io.println("✓ Works!") }
        | false { io.println("✗ Failed, got ${val}") }
}

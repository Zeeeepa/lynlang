// Test Option struct creation
{ io, Option , get_default_allocator} = @std

main = () void {
    alloc = get_default_allocator()
    io.println("Creating Option.Some(100)")
    opt = Option.Some(100)
    
    io.println("Pattern matching...")
    opt ?
        | Some(val) { io.println("Value: ${val}") }
        | None { io.println("None") }
        
    io.println("Done")
}
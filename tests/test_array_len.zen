{ io, get_default_allocator } = @std

main = () i32 {
    io.println("Creating array...")
    alloc = get_default_allocator()
    arr: Array<i32> = Array.new(alloc, 10, 0)
    io.println("Array created")
    
    io.println("Getting length...")
    len = arr.len()
    io.println("Length: ${len}")
    
    return 0
}
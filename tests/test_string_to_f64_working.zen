{ io, Option , get_default_allocator} = @std

main = () void {
    alloc = get_default_allocator()
    // Test valid float parsing
    s1 = "3.14159"
    result1 = s1.to_f64()
    result1 ?
        | Option.Some(v) { io.println("Parsed '${s1}' as: ${v}") }
        | Option.None { io.println("Failed to parse '${s1}'") }
    
    // Test invalid string
    s2 = "not_a_number"
    result2 = s2.to_f64()
    result2 ?
        | Option.Some(v) { io.println("Parsed '${s2}' as: ${v}") }
        | Option.None { io.println("Failed to parse '${s2}'") }
    
    // Test negative number
    s3 = "-42.5"
    result3 = s3.to_f64()
    result3 ?
        | Option.Some(v) { io.println("Parsed '${s3}' as: ${v}") }
        | Option.None { io.println("Failed to parse '${s3}'") }
    
    // Test integer
    s4 = "100"
    result4 = s4.to_f64()
    result4 ?
        | Option.Some(v) { io.println("Parsed '${s4}' as: ${v}") }
        | Option.None { io.println("Failed to parse '${s4}'") }
}
// Test multiple None pattern matches
{ io , get_default_allocator} = @std

main = () void {
    alloc = get_default_allocator()
    empty1 = None
    empty2 = None
    
    io.println("Testing first None")
    empty1 ?
        | None { io.println("First is None") }
        | Some(x) { io.println("First is Some") }
    
    io.println("Testing second None")
    empty2 ?
        | None { io.println("Second is None") }
        | Some(y) { io.println("Second is Some") }
}
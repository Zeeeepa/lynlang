// Test float comparison with raise
{ io, Result } = @std

main = () i32 {
    divide = (a: f64, b: f64) Result<f64, string> {
        b == 0.0 ?
            | true { return Result.Err("Division by zero") }
            | false { return Result.Ok(a / b) }
    }
    
    val = divide(10.0, 2.0).raise()
    io.print("Result: ")
    io.print_float(val)
    io.println("")
    
    return 0
}
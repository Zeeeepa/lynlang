// Simple test to verify Result enum string handling
{ io } = @std

// Define Result type as per spec  
Result<T, E>: Ok(T) | Err(E)

main = () void {
    // Test Err with string
    err_val = Err("error message")
    err_val ?
        | Ok(v) { io.println("Got Ok: ${v}") }
        | Err(e) { io.println("Got Err: ${e}") }
}
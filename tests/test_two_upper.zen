// Test two to_upper calls
{ io } = @std

main = () i32 {
    test1 = "hello".to_upper()
    test2 = "world".to_upper()
    
    test1_ok = test1 == "HELLO"
    test2_ok = test2 == "WORLD"
    
    test1_ok ? {
        io.println("test1 ok")
    }
    test2_ok ? {
        io.println("test2 ok")
    }
    
    return 0
}
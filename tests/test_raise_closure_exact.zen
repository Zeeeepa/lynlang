// Exact copy of failing divide case
{ io, Result } = @std

main = () i32 {
    // Test 2: Closure with parameters returning Result<f64, string>
    divide = (a: f64, b: f64) Result<f64, string> {
        result = b == 0.0 ?
            | true { Result.Err("Division by zero") }
            | false { Result.Ok(a / b) }
        return result
    }
    
    val2 = divide(10.0, 2.0).raise()
    val2 == 5.0 ?
        | true { io.println("✓ Test 2: Closure with params returning Result<f64, string>") }
        | false { io.println("✗ Test 2 failed: Expected 5.0, got ${val2}") }
    
    return 0
}
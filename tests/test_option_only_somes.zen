// Test only Some values, no None
{ io } = @std

main = () i32 {
    io.println("Test 1: Some(42)")
    x = Some(42)
    x ?
        | .Some(a) {
            io.println("Got a: ${a}")
        }
        | .None {
            io.println("None")
        }
    
    io.println("Test 2: Some(100)")  
    z = Some(100)
    z ?
        | .Some(c) {
            io.println("Got c: ${c}")
        }
        | .None {
            io.println("None")
        }
    
    io.println("All tests done")
    0
}
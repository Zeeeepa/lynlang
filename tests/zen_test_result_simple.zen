// Simple Result type test

{ io } = @std

// Import Result and Option from stdlib
{ Result, Ok, Err } = import("../stdlib/core/result.zen")
{ Option, Some, None } = import("../stdlib/core/option.zen")

test_result = () Result<i32, string> {
    return Ok(42)
}

main = () void {
    result = test_result()
    result ?
        | Ok(val) { io.println("Got value") }
        | Err(e) { io.println("Got error") }
}
// Simple Result type test

{io, Result} = @std

// Import Result and Option from stdlib
{io, Result} = @std

test_result = () Result<i32, string> {
    return Result.Ok(42)
}

main = () void {
    result = test_result()
    result ?
        | Result.Ok(val) { io.println("Got value") }
        | Result.Err(e) { io.println("Got error") }
}
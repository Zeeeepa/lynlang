// Test min, max, abs functions from std
{ min, max, abs } = @std

main = () i32 {
    a = 10
    b = 20
    
    smallest = min(a, b)
    largest = max(a, b)
    
    negative = -15
    positive = abs(negative)
    
    result = smallest + largest + positive  // 10 + 20 + 15 = 45
    return result
}
// Test .raise() with Result<i64, string>
{ io, Result , get_default_allocator} = @std

calculate_big = () Result<i64, string> {
    big_num = Result.Ok(1000000)
    val = big_num.raise()
    
    // Do arithmetic with i64
    doubled = val * 2
    
    return Result.Ok(doubled)
}

main = () i32 {
    alloc = get_default_allocator()
    outcome = calculate_big()
    outcome ?
        | Result.Ok(v) { 
            io.println("i64 calculation result: ${v}")
            return 0
        }
        | Result.Err(e) { 
            io.println("Error: ${e}")
            return 1
        }
}
fn add(x: i32, y: i32) i32 {
    return x + y;
}

fn divide(a: f64, b: f64) Result<f64, StaticString> {
    if b == 0.0 {
        return Result.Err("Division by zero");
    }
    return Result.Ok(a / b);
}

fn main() void {
    let result = add(10, 20);
    let quotient = divide(100.0, 5.0);

    let x = 42;
    let y = 3.14;
    let name = "Alice";
}

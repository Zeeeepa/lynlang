// Test .raise() with Result<f64, string>
{ io, Result, Ok, Err } = @std

calculate_pi = () Result<f64, string> {
    pi_result = Ok(3.14159)
    val = pi_result.raise()
    
    // Do arithmetic with f64
    doubled = val * 2.0
    
    return Ok(doubled)
}

main = () i32 {
    outcome = calculate_pi()
    outcome ?
        | Ok(v) { 
            io.println("f64 calculation result: ${v}")
            return 0
        }
        | Err(e) { 
            io.println("Error: ${e}")
            return 1
        }
}
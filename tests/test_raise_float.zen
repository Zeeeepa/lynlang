// Test .raise() with Result<f64, StaticString>
{ io, Result , get_default_allocator} = @std

calculate_pi = () Result<f64, StaticString> {
    pi_result = Result.Ok(3.14159)
    val = pi_result.raise()
    
    // Do arithmetic with f64
    doubled = val * 2.0
    
    return Result.Ok(doubled)
}

main = () i32 {
    alloc = get_default_allocator()
    outcome = calculate_pi()
    outcome ?
        | Result.Ok(v) { 
            io.println("f64 calculation result: ${v}")
            return 0
        }
        | Result.Err(e) { 
            io.println("Error: ${e}")
            return 1
        }
}
{ io, Option } = @std

main = () void {
    s1 = "3.14159"
    result1 = s1.to_f64()
    
    // Test with literal float for comparison
    f = 3.14159
    io.println("Literal float: ${f}")
    
    // Now test the parsed value
    result1 ?
        | Option.Some(parsed) { 
            io.println("Parsed value should be 3.14159")
            // Try arithmetic to see what we get
            doubled = parsed * 2.0
            io.println("Doubled: ${doubled}")
        }
        | Option.None { 
            io.println("Parse failed")
        }
}
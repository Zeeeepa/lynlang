// Debug test for string comparison after case conversion
{ io } = @std

main = () i32 {
    io.println("Testing string comparison...")
    
    test1 = "hello".to_upper()
    io.println(test1)
    
    test1_ok = test1 == "HELLO"
    test1_ok == true ? {
        io.println("test1 comparison passed")
    }
    test1_ok == false ? {
        io.println("test1 comparison failed")
    }
    
    test2 = "WORLD".to_lower()
    io.println(test2)
    
    test2_ok = test2 == "world"
    test2_ok == true ? {
        io.println("test2 comparison passed")
    }
    test2_ok == false ? {
        io.println("test2 comparison failed")
    }
    
    return 0
}
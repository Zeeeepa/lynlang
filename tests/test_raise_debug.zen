// Debug test for .raise() 
{ io, Result, Ok, Err } = @std

main = () i32 {
    // Direct test without intermediate function
    result = Ok(10)
    
    // Extract value with .raise()
    value = result.raise()
    
    // Try to use the value in arithmetic
    doubled = value * 2
    
    io.println("Doubled: ${doubled}")
    return 0
}
{io, get_default_allocator, Array} = @std

main = () i32 {
    alloc = get_default_allocator()
    arr: Array<i32> = Array.new(alloc, 10, 0)
    
    arr.push(42)
    io.println("Pushed 42")
    
    // Don't use StaticString interpolation
    val = arr.get(0)
    io.print_int(val)
    io.println("")
    
    return 0
}
// Test HashMap and Array together
{ io, get_default_allocator, HashMap } = @std

main = () void {
    io.println("Testing HashMap and Array")
    
    alloc = get_default_allocator()
    
    map = HashMap<i32, i32>(alloc)
    arr: Array<i32> = Array.new(alloc, 10, 0)
    
    map.insert(1, 100)
    arr.push(99)
    
    io.println("Both operations completed")
}
// Test nested Result without raise
{ io, Result } = @std

// Returns Result<Result<i32, string>, string>
get_nested = () Result<Result<i32, string>, string> {
    Result.Ok(Result.Ok(42))
}

main = () i32 {
    outer = get_nested()
    
    // Extract inner through pattern matching
    outer ?
        | Result.Ok(inner) {
            // Now try to match on inner directly
            inner ?
                | Result.Ok(val) {
                    io.println("Inner value: ${val}")
                    val == 42 ? | true => 0 | false => 1
                }
                | Result.Err(e) {
                    io.println("Inner error: ${e}")
                    2
                }
        }
        | Result.Err(e) {
            io.println("Outer error: ${e}")
            3
        }
}
// Minimal test for closure returning Result
{ Result, io } = @std

main = () i32 {
    // Test 1: Direct Result.Ok
    res1 = Result.Ok(42)
    val1 = res1.raise()
    io.println("Direct Ok: ${val1}")
    
    // Test 2: Closure returning Result.Ok
    make_result = () {
        return Result.Ok(99)
    }
    res2 = make_result()
    val2 = res2.raise()
    io.println("Closure Ok: ${val2}")
    
    // Test 3: Closure with parameters
    add_result = (a: i32, b: i32) {
        return Result.Ok(a + b)
    }
    res3 = add_result(10, 5)
    val3 = res3.raise()
    io.println("Add result: ${val3}")
    
    // Test comparison
    comp = val3 == 15
    io.println("Compare: ${comp}")
    
    return 0
}
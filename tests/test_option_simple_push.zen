// Minimal test for Array<Option<i32>> push
{ io, Option, Array , get_default_allocator} = @std

main = () void {
    alloc = get_default_allocator()
    io.println("Creating Array<Option<i32>>")
    arr = Array<Option<i32>>()
    
    io.println("Creating Option.Some(42)")
    opt = Option.Some(42)
    
    io.println("Pushing to array...")
    arr.push(opt)
    
    io.println("Success!")
}
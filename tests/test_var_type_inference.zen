// Test variable type inference
{ io, Result , get_default_allocator} = @std

main = () void {
    alloc = get_default_allocator()
    io.println("Test: Variable type inference")
    
    // Store nested result in variable
    nested_result = Result.Ok(Result.Ok(600))
    
    // Extract through variable
    inner_result = nested_result.raise()
    value = inner_result.raise()
    
    io.println("Got value: ${value}")
}
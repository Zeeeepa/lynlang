// Test simple raise type inference
{ io, Result, get_default_allocator} = @std

main = () i32 {
    alloc = get_default_allocator()
    
    // Simple function returning Result  
    get_five = () Result<f64, string> {
        return Result.Ok(5.0)
    }
    
    // Get the value using raise
    val = get_five().raise()
    
    // Direct comparison - should work
    val == 5.0 ? 
        | true { io.println("Pass") }
        | false { io.println("Fail") }
    
    return 0
}
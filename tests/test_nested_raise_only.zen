// Test just the raise part without functions
{ io } = @std
{ Result } = @std

main = () void {
    // Create nested Result directly
    inner = Result.Ok(555)
    outer = Result.Ok(inner)
    
    io.println("Created nested Result")
    
    // Extract using raise
    extracted = outer.raise()
    io.println("Extracted with raise")
    
    // Pattern match on extracted
    extracted ?
        | Result.Ok(val) {
            io.println("Value: ${val}")  // Should be 555
        }
        | Result.Err(e) {
            io.println("Error: ${e}")
        }
}
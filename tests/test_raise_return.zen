// Debug test for .raise() return value
{ io, Result } = @std

test_direct = () Result<i32, string> {
    result = Result.Ok(42)
    value = result.raise()
    return Result.Ok(value)
}

main = () i32 {
    test_result = test_direct()
    test_result ?
        | Result.Ok(value) {
            io.println("Value: ${value}")
            return 0  // Return 0 for test success
        }
        | Result.Err(e) {
            io.println("Error: ${e}")
            return 1
        }
}
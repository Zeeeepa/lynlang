// Test Option with arithmetic
{ io } = @std

main = () i32 {
    // Test with arithmetic in Some
    calc = Some(10 + 15)
    io.println("Created Some(25)")
    
    result_calc = calc ?
        | Some(x) => {
            io.println("x = ${x}")
            x * 2
        }
        | None => 0
        
    io.println("Some(25) * 2 = ${result_calc}")
    
    0
}
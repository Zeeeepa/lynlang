// Test with intermediates
{ io } = @std

main = () i32 {
    hello_str = "hello"
    world_str = "WORLD"
    
    test1 = hello_str.to_upper()
    test2 = world_str.to_lower()
    
    expected1 = "HELLO"
    expected2 = "world"
    
    test1_ok = test1 == expected1
    test2_ok = test2 == expected2
    
    test1_ok ? {
        io.println("test1 ok")
    }
    test2_ok ? {
        io.println("test2 ok")
    }
    
    return 0
}
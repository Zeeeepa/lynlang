// Test simple method calling without traits
{ io } = @std

MyType: {
    value: i32,
}

// Method that takes self as first parameter
print_value = (self: MyType) void {
    io.println("Value: ${self.value}")
}

main = () void {
    obj ::= MyType { value: 42 }
    
    // Call method with explicit parameter
    print_value(obj)
    
    // UFC should work too
    obj.print_value()
}
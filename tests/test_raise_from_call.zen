// Test .raise() from function call result
{ io } = @std

get_result = () Result<i32, string> {
    return Result.Ok(42)
}

main = () i32 {
    // Test direct call and raise
    value = get_result().raise()
    
    // Test arithmetic to verify it's loaded correctly
    test = value + 0
    
    test == 42 ? {
        io.println("SUCCESS: Value is 42")
    }
    
    test != 42 ? {
        io.println("FAIL: Value is not 42")
    }
    
    io.println("Value: ${value}")
    
    return 0  // Return 0 for test success (value was 42 which is correct)
}
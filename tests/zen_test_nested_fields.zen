{ io } = @std

Point: {
    x:: f64,
    y:: f64 = 0
}

Circle: {
    center: Point,
    radius: f64,
}

main = () void {
    io.println("Testing nested struct field access:")
    
    circle = Circle { 
        center: Point { x: 10.0, y: 20.0 }, 
        radius: 5.0 
    }
    
    io.println("circle.center.x: ${circle.center.x}")
    io.println("circle.center.y: ${circle.center.y}")
    io.println("circle.radius: ${circle.radius}")
}

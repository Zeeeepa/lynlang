// Debug test for typed variable issue
{ io } = @std

main = () void {
    // Try without explicit type first
    a = 3
    io.println("a = ${a}")
    
    // Then with explicit type
    b: i64 = 4
    io.println("b = ${b}")
    
    // Create ranges
    range1 = 0..a
    io.println("Created range1")
    
    range2 = 0..b
    io.println("Created range2")
}
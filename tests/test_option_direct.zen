// Direct test of Option pattern matching
{ io, Option , get_default_allocator} = @std

main = () i32 {
    alloc = get_default_allocator()
    test ::= Option.Some(123)
    
    test ?
        | Option.Some(x) { 
            io.println("x = ${x}")
            return 0
        }
        | Option.None { 
            io.println("None")
            return 1
        }
}
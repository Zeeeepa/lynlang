// Simple test to debug parsing
{ io , get_default_allocator, HashMap } = @std

main = () void {
    alloc = get_default_allocator()
    io.println("Before hashmap")
    // Create actual HashMap instead of number
    map = HashMap<i32, i32>(10, alloc)
    io.println("HashMap created")
    // Now use insert as a method
    map.insert(1, 100)
    io.println("Done")
}
// Test Vec<T, size> with generics
{ io , get_default_allocator} = @std

main = () void {
    alloc = get_default_allocator()
    // Create a Vec with fixed size
    vec = Vec<i32, 10>()
    vec.push(42)
    vec.push(100)
    
    io.println("Vec created and items pushed")
    
    // Test access with explicit type
    val: i32 = vec.get(0)
    io.println("vec[0] = ${val}")
}

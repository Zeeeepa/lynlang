// Test Option type with return
{ io } = @std

Option<T>: Some(T) | None

// Function returning Option
find_first = (n: i32) Option<i32> {
    n > 0 ?
        | true { return Some(n) }
        | false { return None }
}

main = () void {
    result = find_first(5)
    result ?
        | Some(val) { io.println("Found: ${val}") }
        | None { io.println("Not found") }
}

{ io, Option } = @std

main = () void {
    // Create an Option.Some(3.14) directly to test pattern matching
    opt1 = Option.Some(3.14)
    opt1 ?
        | Option.Some(v) { io.println("Direct Option.Some(3.14) gives: ${v}") }
        | Option.None { io.println("ERROR: Got None") }
    
    // Now test to_f64
    s1 = "3.14"
    opt2 = s1.to_f64()
    opt2 ?
        | Option.Some(v) { io.println("s1.to_f64() gives: ${v}") }
        | Option.None { io.println("s1.to_f64() gives None") }
}
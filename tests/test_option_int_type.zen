{io, Option} = @std

main = () i32 {
    // Test with explicit i32
    x: i32 = 42
    opt = Option.Some(x)
    
    opt ?
        | Option.Some(val) {
            // val should be i32
            result = val + 10
            io.println("Result: ${result}")
        }
        | Option.None {
            io.println("None") 
        }
    
    return 0
}
{ io, Array } = @std

main = () i32 {
    // Create array
    arr: Array<i32> = Array.new(10, 999)  // Use 999 as default
    
    io.println("Created array with default 999")
    
    // Check initial values
    io.println("Before any push:")
    io.println("  arr[0] = ${arr.get(0)}")
    io.println("  arr[1] = ${arr.get(1)}")
    io.println("  arr[2] = ${arr.get(2)}")
    io.println("  arr[3] = ${arr.get(3)}")
    
    // Push values
    arr.push(100)
    io.println("\nAfter push(100):")
    io.println("  arr[0] = ${arr.get(0)}")
    io.println("  arr[1] = ${arr.get(1)}")
    
    arr.push(200)
    io.println("\nAfter push(200):")
    io.println("  arr[0] = ${arr.get(0)}")
    io.println("  arr[1] = ${arr.get(1)}")
    io.println("  arr[2] = ${arr.get(2)}")
    
    arr.push(300)
    io.println("\nAfter push(300):")
    io.println("  arr[0] = ${arr.get(0)}")
    io.println("  arr[1] = ${arr.get(1)}")
    io.println("  arr[2] = ${arr.get(2)}")
    io.println("  arr[3] = ${arr.get(3)}")
    
    return 0
}
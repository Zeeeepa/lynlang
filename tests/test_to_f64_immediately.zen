{ io, Option , get_default_allocator} = @std

main = () void {
    alloc = get_default_allocator()
    // Pattern match immediately after to_f64()
    "3.14159".to_f64() ?
        | Option.Some(v) {
            io.println("Value: ${v}")
        }
        | Option.None {
            io.println("Parse failed")
        }
}
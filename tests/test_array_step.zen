{ io, Array } = @std

main = () i32 {
    io.println("1. Creating array...")
    arr: Array<i32> = Array.new(10, 0)
    
    io.println("2. Pushing value...")
    arr.push(100)
    
    io.println("3. Calling get...")
    val = arr.get(0)
    
    io.println("4. Got value")
    
    io.println("5. Done")
    return 0
}
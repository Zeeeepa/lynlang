// Debug HashMap with allocator
{ io, get_default_allocator, HashMap } = @std

main = () void {
    io.println("Test 1: Get allocator")
    alloc = get_default_allocator()
    io.println("Got allocator")
    
    io.println("Test 2: Create HashMap")
    map = HashMap<i32, i32>(alloc)
    io.println("Created HashMap")
    
    io.println("Test 3: Insert to HashMap")
    map.insert(1, 100)
    io.println("Inserted to HashMap")
    
    io.println("All tests passed")
}
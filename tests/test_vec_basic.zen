// Test Vec<T, size> basic operations
{ io , get_default_allocator} = @std

main = () void {
    alloc = get_default_allocator()
    // Create a Vec with fixed size
    vec = Vec<i32, 10>()
    vec.push(42)
    
    io.println("Vec created and pushed")
    
    // Test len
    length = vec.len()
    io.println("Vec length: ${length}")
}

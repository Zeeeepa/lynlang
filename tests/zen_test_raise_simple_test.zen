// Simple test for .raise()
{ io } = @std

Result<T, E>: Ok(T) | Err(E)

get_value = () Result<i32, string> {
    return Ok(42)
}

use_raise = () Result<i32, string> {
    val = get_value().raise()  // Should extract 42 from Ok(42)
    return Ok(val)
}

main = () i32 {
    result = use_raise()
    result ?
        | Ok(v) { 
            io.println("Got value: ${v}")
            return 0
        }
        | Err(e) {
            io.println("Error: ${e}")
            return 1
        }
}
// Test closure with params and type
{ io, Result } = @std

main = () i32 {
    // Closure with params and explicit return type
    test = (x: f64) Result<f64, string> {
        return Result.Ok(x * 2.0)
    }
    
    // Call it  
    result = test(5.0)
    
    // Check the result type
    result ?
        | Result.Ok(v) { 
            io.println("Ok: ${v}")
            // Now check if v can be compared
            check = v == 10.0
            io.println("v == 10.0? ${check}")
        }
        | Result.Err(e) { io.println("Err: ${e}") }
    
    return 0
}
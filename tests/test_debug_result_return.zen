// Debug test for Result return type in conditionals
{ io, Result } = @std

simple_test = () Result<i32, string> {
    x = 1
    x == 1 ?
        | true { return Result.Ok(42) }
        | false { return Result.Err("error") }
}

main = () i32 {
    r = simple_test()
    io.println("Test complete")
    return 0
}
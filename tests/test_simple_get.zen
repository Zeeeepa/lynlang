// Simplest possible test for DynVec.get
{io, DynVec, get_default_allocator} = @std

main = () i32 {
    alloc = get_default_allocator()
    vec = DynVec<i32>(alloc)
    vec.push(42)
    
    // Direct pattern match without variable
    vec.get(0) ?
        | Some(v) { return v }
        | None { return -1 }
}
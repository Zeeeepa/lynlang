// Debug test for string case conversion
{ io } = @std

main = () i32 {
    io.println("Starting test...")
    
    // Test 1: Basic uppercase conversion
    io.println("Test 1...")
    lower_str = "hello world"
    upper = lower_str.to_upper()
    io.println(upper)
    
    // Test 2: Basic lowercase conversion
    io.println("Test 2...")
    upper_str = "HELLO WORLD"
    lower = upper_str.to_lower()
    io.println(lower)
    
    // Test 3: Mixed case conversion
    io.println("Test 3...")
    mixed = "Hello World 123!"
    mixed_upper = mixed.to_upper()
    io.println("After to_upper...")
    mixed_lower = mixed.to_lower()
    io.println(mixed_upper)
    io.println(mixed_lower)
    
    // Test 4: Empty string
    io.println("Test 4...")
    empty = ""
    empty_upper = empty.to_upper()
    empty_lower = empty.to_lower()
    io.println("Empty done")
    
    // Test 5: Chain method calls
    io.println("Test 5...")
    chain_test = "hello"
    chain_result = chain_test.to_upper().to_lower()
    io.println(chain_result)
    
    io.println("All tests completed!")
    return 0
}
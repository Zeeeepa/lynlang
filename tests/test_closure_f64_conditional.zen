// Test closure type with f64 conditional  
{ io, Result } = @std

main = () i32 {
    // Test with f64 conditional - exactly like the failing test
    divide = (a: f64, b: f64) Result<f64, StaticString> {
        b == 0.0 ?
            | true { return Result.Err("Division by zero") }
            | false { return Result.Ok(a / b) }
    }
    
    v1 = divide(10.0, 2.0).raise()
    io.print_float(v1)
    io.println("")
    
    // This comparison fails with "Cannot compare types Void and F64"
    v1 == 5.0 ?
        | true { io.println("v1 == 5.0: OK") }
        | false { io.println("v1 == 5.0: FAILED") }
    
    return 0
}
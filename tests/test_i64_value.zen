{ io, Array , get_default_allocator} = @std

main = () i32 {
    alloc = get_default_allocator()
    arr: Array<i32> = Array.new(alloc, 10, 0)
    
    // Push a specific value
    arr.push(333)
    
    io.println("Pushed 333")
    
    // Get it back
    val = arr.get(0)
    io.println("arr.get(0) = ${val}")
    
    return 0
}
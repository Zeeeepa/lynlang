// Simple test to debug nested struct field access
{ io } = @std

Point: {
    x: f64,
    y: f64
}

Rectangle: {
    top_left: Point,
    bottom_right: Point,
}

main = () void {
    rect = Rectangle {
        top_left: Point { x: 0.0, y: 0.0 },
        bottom_right: Point { x: 20.0, y: 10.0 }
    }
    
    // Test direct field access
    tl = rect.top_left
    br = rect.bottom_right
    
    io.println("top_left.x: ${tl.x}")
    io.println("top_left.y: ${tl.y}")
    io.println("bottom_right.x: ${br.x}")
    io.println("bottom_right.y: ${br.y}")
    
    // Test nested field access
    io.println("rect.top_left.x: ${rect.top_left.x}")
    io.println("rect.top_left.y: ${rect.top_left.y}")
    io.println("rect.bottom_right.x: ${rect.bottom_right.x}")
    io.println("rect.bottom_right.y: ${rect.bottom_right.y}")
}
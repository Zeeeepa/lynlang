// Debug DynVec with allocator
{ io, get_default_allocator, DynVec } = @std

main = () void {
    io.println("Test 1: Get allocator")
    alloc = get_default_allocator()
    io.println("Got allocator")
    
    io.println("Test 2: Create DynVec")
    vec = DynVec<i32>(alloc)
    io.println("Created DynVec")
    
    io.println("Test 3: Push to DynVec")
    vec.push(42)
    io.println("Pushed to DynVec")
    
    io.println("All tests passed")
}
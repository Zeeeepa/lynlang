// Test simple Result<i32,string> 
{ io, Result } = @std

main = () void {
    io.println("=== Testing Simple Result<i32,string> ===")
    
    result = Result.Ok(42)
    io.println("Created Result.Ok(42)")
    
    result ?
        | Result.Ok(val) {
            io.println("Value: ${val}")
            val == 42 ?
                | true { io.println("✓ Simple Result works!") }
                | false { io.println("✗ Wrong value: ${val}") }
        }
        | Result.Err(e) { io.println("Error: ${e}") }
}
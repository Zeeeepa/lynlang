{ io } = @std

main = () i32 {
    // Test basic Option with i32 - no arithmetic
    x: i32 = 42
    opt = Option.Some(x)
    
    opt ?
        | Option.Some(val) {
            io.println("Value: ${val}")
        }
        | Option.None {
            io.println("None") 
        }
    
    return 0
}
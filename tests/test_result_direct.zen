// Test Result directly without function return
{ io, Result } = @std

main = () i32 {
    io.println("Testing Result directly (no function return):")
    
    // Direct Result.Err with string
    err: Result<i32, string> = Result.Err("direct error")
    err ?
        | .Ok(val) { io.println("Ok: ${val}") }
        | .Err(msg) { io.println("Err: ${msg}") }
    
    // Direct Result.Ok with i32
    ok: Result<i32, string> = Result.Ok(42)
    ok ?
        | .Ok(val) { io.println("Ok: ${val}") }
        | .Err(msg) { io.println("Err: ${msg}") }
    
    return 0
}
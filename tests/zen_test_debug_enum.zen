// Debug test for enum payload types
{ io } = @std

Result<T, E>: Ok(T) | Err(E)

main = () void {
    // Test with integer payload
    err_int = Err(42)
    err_int ?
        | Ok(v) { io.println("Got Ok int: ${v}") }
        | Err(e) { io.println("Got Err int: ${e}") }
    
    // Test with string payload
    err_str = Err("hello")
    err_str ?
        | Ok(v) { io.println("Got Ok str: ${v}") }
        | Err(e) { 
            // Try to use the value directly
            io.println("Got Err str: ${e}")
        }
}
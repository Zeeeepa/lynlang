// Test DynVec.get return type
{io, DynVec, get_default_allocator, Option, Result} = @std

main = () i32 {
    alloc = get_default_allocator()
    
    // Test DynVec<i32>
    vec = DynVec<i32>(alloc)
    vec.push(42)
    
    // This should return Option<i32>
    item = vec.get(0)
    
    item ?
        | Some(v) { io.print_int(v) }
        | None { io.println("None") }
    
    return 0
}
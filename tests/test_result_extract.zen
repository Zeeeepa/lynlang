// Test Result value extraction via pattern matching
{ io, Result } = @std

get_result = () Result<i32, string> {
    return Result.Ok(42)
}

main = () i32 {
    io.println("Testing Result extraction:")
    
    result = get_result()
    result ?
        | Result.Ok(val) { 
            io.println("Extracted value: ${val}")
            io.println("Value + 10 = ${val + 10}")
        }
        | Result.Err(e) { io.println("Error: ${e}") }
    
    return 0
}
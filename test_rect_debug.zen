// Test rectangle debug
{ io } = @std

Point: {
    x: f64,
    y: f64,
}

Rectangle: {
    top_left: Point,
    bottom_right: Point,
}

main = () void {
    io.println("Creating rectangle...")
    
    tl = Point { x: 0.0, y: 0.0 }
    br = Point { x: 10.0, y: 5.0 }
    
    io.println("Top-left: (${tl.x}, ${tl.y})")
    io.println("Bottom-right: (${br.x}, ${br.y})")
    
    rect = Rectangle {
        top_left: tl,
        bottom_right: br
    }
    
    io.println("Rectangle top-left: (${rect.top_left.x}, ${rect.top_left.y})")
    io.println("Rectangle bottom-right: (${rect.bottom_right.x}, ${rect.bottom_right.y})")
    
    width: f64 = rect.bottom_right.x - rect.top_left.x
    height: f64 = rect.bottom_right.y - rect.top_left.y
    area: f64 = width * height
    
    io.println("Width: ${width}, Height: ${height}, Area: ${area}")
}
// Test the consolidated collections structure
// This tests that all the imports work correctly

// Test importing vec directly
vec_module = import("stdlib/vec")

// Test importing collections
collections_module = import("stdlib/collections") 

// Test importing individual collection types
list_module = import("stdlib/list")
hashmap_module = import("stdlib/hashmap")
set_module = import("stdlib/set")
stack_module = import("stdlib/stack")
queue_module = import("stdlib/queue")
array_module = import("stdlib/array")

// Test creating some basic types
main = () {
    // Test Vec types are available
    fixed_vec := vec_module.Vec<i32, 10>.new()
    dyn_vec := vec_module.DynVec<i32>.init(@std.default_allocator())
    
    // Test collections types are available  
    stack := collections_module.Stack<i32>.new()
    queue := collections_module.Queue<i32>.new()
    
    // Test standalone imports work
    list := list_module.new<i32>()
    hashmap := hashmap_module.hashmap_new<string, i32>()
    
    // Test array utilities
    arr := [1, 2, 3, 4, 5]
    vec_from_array := array_module.array_to_vec(arr)
    
    println("Consolidated collections test passed!")
}

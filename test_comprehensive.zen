main = () void {
    @std.io.println("=== Zen Compiler Test ===")
    
    // Numbers
    x = 42
    y = 7
    
    // Arithmetic
    sum = x + y
    diff = x - y  
    prod = x * y
    quot = x / y
    
    @std.io.println("Numbers:")
    @std.io.println(x)
    @std.io.println(y)
    
    @std.io.println("Arithmetic:")
    @std.io.println(sum)
    @std.io.println(diff)
    @std.io.println(prod)
    @std.io.println(quot)
    
    // Booleans
    is_true = true
    is_false = false
    
    @std.io.println("Booleans:")
    @std.io.println(is_true)
    @std.io.println(is_false)
    
    // Strings
    greeting = "Hello World"
    @std.io.println("String:")
    @std.io.println(greeting)
    
    @std.io.println("=== Test Complete ===")
}
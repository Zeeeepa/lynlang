{ io, Array } = @std

main = () void {
    io.println("Creating array...")
    arr: Array<i32> = Array.new(10, 0)
    io.println("Array created")
    
    // Push some values first
    arr.push(10)
    arr.push(20)
    arr.push(30)
    
    io.println("Length after pushes: ${arr.len()}")
    
    // Now try to get
    val0 = arr.get(0)
    io.println("Value at 0: ${val0}")
    
    // Now try to set
    arr.set(0, 100)
    val0_new = arr.get(0)
    io.println("Value at 0 after set: ${val0_new}")
}
